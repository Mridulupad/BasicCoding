package com.mridul.coding;

import java.util.Arrays;

public class MissingNumber {

	public static void main(String[] args) {
		int[] input = { 1, 1, 2, 3, 5, 5, 7, 9, 9, 9 }; // let's create another array with same length // by default all index will contain zero // default value for int variable int[] register = new int[input.length]; // now let's iterate over given array to // mark all present numbers in our register // array for (int i : input) { register[i] = 1; }

		int[] register = new int[input.length]; // now let's iterate over given array to // mark all present numbers in our register // array for (int i : input) { register[i] = 1; }

		for (int i=0;i<input.length ; i++) { 
		System.out.println(input[i]);
			register[input[i]] = 1; 
//		System.out.println(register[i]);
			}
	System.out.println(Arrays.toString(register));

		for(int i=1;i<register.length;i++) {
			if(register[i]==0) {
//				System.out.println(i);
			}
		}
		
	}
}
